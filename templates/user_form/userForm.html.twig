{% extends 'home/index.html.twig' %}
{% block body %}
    <h1>Add User</h1>

    {{ form_start(form) }}
        {{ form_row(form.email) }}
        {{ form_row(form.password) }}
        {{ form_row(form.username) }}
        <button type="submit" class="btn btn-success">Add User</button>
    {{ form_end(form) }}
{% endblock %}
